{
	//"C_Cpp.clang_format_path": "C:/Program Files/LLVM/bin/clang-format.exe",
	"C_Cpp.clang_format_style": "Visual Studio",
	"[cpp]": {
		"editor.quickSuggestions": true
	},
	"[c]": {
		"editor.quickSuggestions": true
	},
	"editor.formatOnSave": false,
	"editor.tabSize": 4,
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"editor.wordWrap": "on",
	"files.trimTrailingWhitespace": true,
	"files.associations": {
		"thread": "cpp",
		"array": "cpp",
		"initializer_list": "cpp",
		"utility": "cpp",
		"system_error": "cpp",
		"functional": "cpp",
		"type_traits": "cpp",
		"tuple": "cpp",
		"xstring": "cpp",
		"iosfwd": "cpp",
		"xlocale": "cpp",
		"*.tcc": "cpp",
		"bitset": "cpp",
		"cctype": "cpp",
		"clocale": "cpp",
		"cmath": "cpp",
		"cstdint": "cpp",
		"cstdio": "cpp",
		"cstdlib": "cpp",
		"cstring": "cpp",
		"ctime": "cpp",
		"cwchar": "cpp",
		"cwctype": "cpp",
		"deque": "cpp",
		"unordered_map": "cpp",
		"vector": "cpp",
		"exception": "cpp",
		"fstream": "cpp",
		"iomanip": "cpp",
		"iostream": "cpp",
		"istream": "cpp",
		"limits": "cpp",
		"memory": "cpp",
		"new": "cpp",
		"ostream": "cpp",
		"numeric": "cpp",
		"sstream": "cpp",
		"stdexcept": "cpp",
		"streambuf": "cpp",
		"typeinfo": "cpp"
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/*.old": true,
		"**/*.rst": true,
		"**/build": true,
		"**/UnitTest Wifi + RFID": true,
		"**/CVS": true,
		"**/.DS_Store": true
	  },
	"C_Cpp.intelliSenseEngine":"Default"
}